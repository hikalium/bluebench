{
  "name": "bluebench",
  "description": "A ChromiumOS benchmark from browser perspective",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "power",
    "scripting",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage",
    "tabs"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "file://*/",
    "http://*/",
    "https://*/"
  ]
}
